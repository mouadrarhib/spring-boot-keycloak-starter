{
  "realm": "spring-boot-realm",
  "enabled": true,
  "displayName": "Spring Boot Realm",
  "roles": {
    "realm": [
      {
        "name": "USER",
        "description": "Standard user role"
      },
      {
        "name": "ADMIN",
        "description": "Administrator role"
      }
    ]
  },
  "clients": [
    {
      "clientId": "spring-boot-api",
      "name": "Spring Boot Resource Server",
      "rootUrl": "http://localhost:8080",
      "adminUrl": "http://localhost:8080",
      "baseUrl": "http://localhost:8080",
      "surrogateAuthRequired": false,
      "enabled": true,
      "redirectUris": [
        "http://localhost:8080/*",
        "http://localhost:*/*"
      ],
      "webOrigins": [
        "*"
      ],
      "protocol": "openid-connect",
      "attributes": {
        "tls.client.certificate.bound.access.tokens": "false"
      },
      "publicClient": false,
      "bearerOnly": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "authorizationServicesEnabled": false,
      "clientAuthenticatorType": "client-secret",
      "secret": "secret123",
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "profile",
        "roles",
        "email"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access",
        "microprofile-jwt"
      ]
    }
  ],
  "users": [
    {
      "username": "user1",
      "enabled": true,
      "emailVerified": false,
      "credentials": [
        {
          "type": "password",
          "value": "user1pass",
          "temporary": false
        }
      ],
      "realmRoles": [
        "USER"
      ]
    },
    {
      "username": "admin1",
      "enabled": true,
      "emailVerified": false,
      "credentials": [
        {
          "type": "password",
          "value": "admin1pass",
          "temporary": false
        }
      ],
      "realmRoles": [
        "USER",
        "ADMIN"
      ]
    }
  ]
}